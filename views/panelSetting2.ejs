    <script type="text/javascript">
        $(document).ready(function() {
            $('.selectpicker').select2({
                minimumResultsForSearch: Infinity
            });
        });

        $.ajax({
            type: "POST",
            url: "panelSetting/get_equipments",
            async: false,
            dataType: "json",
            success: function (data) { 
                for(var i=0; i<data.length; i++) {
                    $('#equipment_container').append(`<div class="col-3 middle text-center">
                                                        <div class="equipment_box" draggable="true" data-id="`+data[i]['name']+`" style="width: 101px; height: 151px;">
                                                            <img src="`+data[i]['icon_path']+`" style="height: 70%">
                                                            <p class="m-0" style="height: 30%">`+data[i]['name']+`</p>
                                                        </div>
                                                    </div>`)
                }
            },
            error: function (e) {
                console.info(e);
                console.info("Error");
            },
            done: function (e) {
                console.info("DONE");
            }
        });

        

    </script>
