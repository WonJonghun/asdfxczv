    <script type="text/javascript">
        $(document).ready(function() {
            $('.selectpicker').select2({
                minimumResultsForSearch: Infinity
            });
        });

        $('#btn_analytic').on('click', function() {
            test()
        })

        function test() {
            setTimeout(function() {
                $('.loadingBox').removeClass('display-none');
                setTimeout(function() {
                    $('.loadingBox').addClass('display-none');
                }, 3000)
            }, 1000)            
        }
